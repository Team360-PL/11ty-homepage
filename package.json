{
  "name": "e-editiones-homepage",
  "version": "1.0.0",
  "description": "New e-editiones homepage",
  "scripts": {
    "build": "DEBUG=tei-publisher npx @11ty/eleventy",
    "build.all": "DEBUG=tei-publisher TP_NO_CACHE=true npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "watch": "TP_DISABLED=true DEBUG=tei-publisher npx @11ty/eleventy --watch",
    "serve": "TP_DISABLED=true DEBUG=tei-publisher npx @11ty/eleventy --serve --port 8081",
    "start": "npm run serve",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wolfgangmm/11ty-homepage.gi"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@teipublisher/pb-eleventy-plugin": "^0.9.4",
    "eleventy-favicon": "^1.1.3",
    "eleventy-plugin-toc": "^1.1.5",
    "luxon": "^3.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-implicit-figures": "^0.10.0"
  }
}
